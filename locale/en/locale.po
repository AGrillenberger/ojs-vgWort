msgid "plugins.generic.vgwort.displayName"
msgstr "VG Wort Plugin"

msgid "plugins.generic.vgwort.description"
msgstr "This plugin enables integration and management of the <a href="http://www.vgwort.de/" target="_blank">VG Wort</a> pixel tags in OJS. It requires the VG Wort publisher account and the configuration of the plugin settings."


msgid "plugins.generic.vgwort.settings.vgWortUserIdPassword"
msgstr "VG Wort username and password"

msgid "plugins.generic.vgwort.settings.vgWortUserIdPassword.description"
msgstr "Enter VG Wort publisher username and password that will be used for the VG Wort services authentication."

msgid "plugins.generic.vgwort.settings.vgWortUserId"
msgstr "Username"

msgid "plugins.generic.vgwort.settings.vgWortUserPassword"
msgstr "Password"

msgid "plugins.generic.vgwort.settings.vgWortRegistration"
msgstr "Automatic registration"

msgid "plugins.generic.vgwort.settings.vgWortRegistration.description"
msgstr "Specify a date in a year and/or the number of days after the article publication. The unregistered and active pixel tags will then be automatically registered with VG Wort on the specified date every year, or considering the specified amount of time after the article publication. If both options are set and the specified date in a year applies, all pixel tags will be considered regardless of the article publication date. If a pixel tag registration fails, a new attempt will be made to register the pixel tag next time the scheduled task runs."

msgid "plugins.generic.vgwort.settings.vgWortRegistration.dateInYear"
msgstr "Date in a year"

msgid "plugins.generic.vgwort.settings.clearDate"
msgstr "Clear"

msgid "plugins.generic.vgwort.settings.vgWortRegistration.daysAfterPublication"
msgstr "Number of days after publication"

msgid "plugins.generic.vgwort.settings.vgWortTestMode"
msgstr "Test mode"

msgid "plugins.generic.vgwort.settings.vgWortTestMode.description"
msgstr "Use the VG Wort test API (testing environment). Please do not forget to remove this option for the production."


msgid "plugins.generic.vgwort.distribution.pixelTags.tab"
msgstr "Pixel Tags"

msgid "plugins.generic.vgwort.distribution.pixelTags.LocalizationNotice"
msgstr "Please note that the data is transferred in the submission language of the selected article. For individuals to participate in royalty disribution, the spelling of names has to be identical to the spelling as registered with vgWort. The submission langauge can be selected in the article metadata."

